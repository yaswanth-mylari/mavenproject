pipeline{
	agent any
	tools{
	maven 'M3'
	}
	stages{
	stage("checkout"){
		steps{
			git 'https://github.com/yaswanth-mylari/mavenproject.git'
			}
		}
	stage ('Build'){
		steps{
		sh 'mvn clean compile'
		}
	}
	stage ('test'){
	steps{
		sh 'mvn test'
		junit '**/target/surefire-reports/TEST-*.xml'
	}
	}
	stage ('Package')
	{
	steps{
		sh 'mvn package'
	}
	}
	}
}
